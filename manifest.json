{
    "manifest_version": 3,
    "name": "Fooocus Enhancer",
    "version": "1.0.0",
    "description": "TODO",
    "permissions": ["activeTab", "storage"],
    "host_permissions": ["http://127.0.0.1:7865/*"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["http://127.0.0.1:7865/*"],
        "js": ["jquery.min.js", "content.js"]
    }],
    "action": {
        "default_icon": {
            "16": "icon-16.png",
            "48": "icon-48.png",
            "128": "icon-128.png"
        }
    }
}
